FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.2;

deltaT          0.005;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{


	CheckFunction
    {
        type            coded;
        libs            ("libutilityFunctionObjects.so");
        codeWrite
        #{
            // Include necessary headers
            #include "IOobject.H"

            // Print a message to the console
            Info << "CheckFunction is working!" << endl;
        #};
    }	

}
